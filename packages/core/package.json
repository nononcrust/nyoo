{
  "name": "nyoo",
  "type": "module",
  "version": "0.0.5",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint ./src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "build": "tsup",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm test:ci && pnpm lint && pnpm type-check",
    "release": "pnpm publish"
  },
  "dependencies": {
    "next": "^15.3.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "eslint": "^9.24.0",
    "tsup": "^8.4.0",
    "typescript": "5.8.2",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "next": ">=14.0.0"
  }
}
